type User {
	id: ID!
	username: String!
	email: String!

	meetups: [Meetup!]!
}

input NewUser {
  username: String!
  email: String!
}

type Meetup {
	id: ID!
	name: String!
	description: String!
	user: User!
}

input NewMeetup {
  name: String!
  description: String!
  userId: String!
}

type Query {
	meetups: [Meetup!]!
}

type Mutation {
  createMeetup(input: NewMeetup!): Meetup!
  createUser(input: NewUser!): User!
}
